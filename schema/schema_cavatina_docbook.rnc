namespace db = "http://docbook.org/ns/docbook"
default namespace = "http://docbook.org/ns/docbook"

include "docbook.rnc"

db.domain.inlines |= db.computer-key

# Define a new computer-key element:

db.computer-key.role.attribute = attribute role { text }
db.computer-key.attlist =
   db.computer-key.role.attribute?
 & db.common.attributes
 & db.common.linking.attributes

db.computer-key =
   element computer-key {
      db.computer-key.attlist,
      db._text
   }